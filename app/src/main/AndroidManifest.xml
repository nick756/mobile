<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nova.sme.sme01" >
    android:versionCode="31"
    android:versionName="3.1">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-feature android:name="android.hardware.camera"></uses-feature>




    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegularLoginActivity"
            android:label="@string/title_activity_regular_login" >
        </activity>
        <activity
            android:name=".TransactionActivity"
            android:label="@string/title_activity_transaction"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait"
            >

        </activity>
        <activity
            android:name=".TransactionsViewActivity"
            android:label="@string/title_activity_transactions_view" >
        </activity>
        <activity
            android:name=".HelpNActivity"
            android:label="@string/title_activity_help_n" >
        </activity>
        <activity
            android:name=".HelpItemActivity"
            android:label="@string/title_activity_help_item" >
        </activity>
    </application>

</manifest>
